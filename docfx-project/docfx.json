{
  "metadata": [
    //DataLib
    {
      "src": [
        {
          "src": "../DataLib",
          "files": [
            "DataLib.csproj"
          ]
        }
      ],
      "dest": "data",
      "filter": "!ref-filters/data.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    //ServerApp
    {
      "src": [
        {
          "src": "../ServerApp",
          "files": [
            "ServerApp.csproj"
          ]
        }
      ],
      "dest": "server",
      "filter": "!ref-filters/server.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": true
    },
    //UnityClient
    {
      "src": [
        {
          "src": "../UnityClient/Assets/ClientLogic",
          "files": [
            "**/*.cs"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "!ref-filters/client.yml",
      "dest": "client",
      "disableGitFeatures": false,
      "disableDefaultFilter": true
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "efcore-unity-cross-protocol-study",
      "_appFooter": "efcore-unity-cross-protocol-study<br>Generated by <strong>DocFX</strong>",
      "_gitUrlPattern": "github",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "src": "data",
        "files": [ "*.yml" ],
        "dest": "data"
      },
      {
        "src": "server",
        "files": [ "*.yml" ],
        "dest": "server"
      },
      {
        "src": "client",
        "files": [ "*.yml" ],
        "dest": "client"
      }
    ],
    "resource": [
      
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default", "staticdoc" ],
    "xref": [
        "https://nicoco007.github.io/UnityXRefMap/2022.2/xrefmap.yml"
    ],
    "xrefService": [
        "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}